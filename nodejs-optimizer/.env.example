# Environment Configuration for Node.js Optimizer

# Laravel API Configuration
LARAVEL_API_URL=http://localhost:8000/api

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Google Search Configuration (using SerpAPI or Custom Search)
# Option 1: SerpAPI (recommended - easier to use)
SERPAPI_KEY=your_serpapi_key_here

# Option 2: Google Custom Search API (alternative)
# GOOGLE_API_KEY=your_google_api_key_here
# GOOGLE_SEARCH_ENGINE_ID=your_search_engine_id_here

# Scraping Configuration
MAX_COMPETITOR_RESULTS=2
REQUEST_TIMEOUT_MS=30000

# Debug Mode
DEBUG=false
